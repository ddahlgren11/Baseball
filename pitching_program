        <div class="text-center mt-10 p-6 bg-white dark:bg-gray-800 rounded-xl shadow-md">
            <h3 class="text-2xl font-bold text-gray-900 dark:text-white">Day 7: Full Rest</h3>
            <p class="text-lg text-gray-700 dark:text-gray-300 mt-2">
                This is a dedicated rest and recovery day. Focus on hydration, nutrition, and sleep. No throwing.
            </Same>
        </div>

    </div>

    <script>
        document.addEventListener('DOMContentLoaded', () => {
            const allTextAreas = document.querySelectorAll('textarea');
            // Create a list of all textarea IDs
            const textAreaIds = Array.from(allTextAreas).map(t => t.id);

            // 1. Load saved data on page load
            allTextAreas.forEach(textarea => {
                const savedValue = localStorage.getItem(textarea.id);
                if (savedValue) {
                    textarea.value = savedValue;
                }
                
                // 2. Save data on input
                // As the user types, save the new value to localStorage
                textarea.addEventListener('input', (e) => {
                    localStorage.setItem(e.target.id, e.target.value);
                });
            });

            // 3. Clear button functionality
            const clearButton = document.getElementById('clear-edits-btn');
            clearButton.addEventListener('click', () => {
                // Loop through all known textareas and remove their saved data
                textAreaIds.forEach(id => {
                    localStorage.removeItem(id);
                });
                // Reload the page to show the default values from the HTML
                location.reload();
            });

            // 4. Save & Download functionality
            const saveButton = document.getElementById('save-btn');
            saveButton.addEventListener('click', () => {
                // Create a clone of the entire document
                const clone = document.documentElement.cloneNode(true);

                // Find all textareas in the clone
                clone.querySelectorAll('textarea').forEach(clonedTextarea => {
                    // Get the ID of the textarea
                    const id = clonedTextarea.id;
                    // Get the *current* value from the *live* page
                    const currentValue = document.getElementById(id).value;
                    // Set the innerHTML of the *cloned* textarea.
                    // This "burns" the current value into the HTML source.
                    clonedTextarea.innerHTML = currentValue;
                });

                // Get the full HTML of the modified clone
                const newHtml = clone.outerHTML;
                
                // Create a blob (a file-like object)
                const blob = new Blob([newHtml], { type: 'text/html' });
                
                // Create a temporary link element
                const a = document.createElement('a');
a.href = URL.createObjectURL(blob);
                a.download = 'my_pitching_program.html'; // Set the download filename
                
                // Append, click, and remove the link to trigger the download
                document.body.appendChild(a);
                a.click();
                document.body.removeChild(a);
                
                // Clean up the object URL
                URL.revokeObjectURL(a.href);
            });
        });
    </script>

</body>
</html>
